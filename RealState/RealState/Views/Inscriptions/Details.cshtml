@model RealState.ViewModels.InscriptionViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Inscription</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.attention_number)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.attention_number)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.Cne!.type)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.Cne!.type)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.Localization!.comuna)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.Localization!.comuna)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.Localization!.manzana)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.Localization!.manzana)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.Localization!.predio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.Localization!.predio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.fojas)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.fojas)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.date)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.date)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Inscription!.number)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Inscription!.number)
        </dd>
    </dl>
    <h4>Enajenantes</h4>
    <table class="table">
        <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(m => m.Enajenantes![0].Rut)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Enajenantes![0].Percentage_right)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Enajenantes![0].Check_percentage_not_credited)
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.Enajenantes!) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Rut)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Percentage_right)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Check_percentage_not_credited)
                </td>
            </tr>
        }
        </tbody>
    </table>
    <h4>Adquirientes</h4>
    <table class="table">
        <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(m => m.Adquirientes![0].Rut)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Adquirientes![0].Percentage_right)
            </th>
            <th>
                @Html.DisplayNameFor(m => m.Adquirientes![0].Check_percentage_not_credited)
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.Adquirientes!) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Rut)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Percentage_right)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Check_percentage_not_credited)
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Inscription!.id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
